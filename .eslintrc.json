{
    "extends": "airbnb-base",
    "env": {
        "es6": true,
        "node": true,
        "mocha": true
    },
    "rules" : {
        "no-console":  "off",
        "comma-dangle": 0,
        "prefer-const": "warn",
        "no-unused-vars": "warn"

    },
    "overrides": [
        {
            "files": ["*.test.js", "*.spec.js"],
            "rules": {
                "no-unused-expressions": "off"
            }
        }
    ]
}
